    "@xterm/addon-web-links": "^0.10.0",
    "@xterm/addon-webgl": "^0.17.0",
    "@xterm/xterm": "^5.5.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.0.15",
    "immer": "^10.0.4",
    "js-base64": "^3.7.7",
    "jsonpath": "^1.1.1",
    "lodash.get": "^4.4.2",
    "monaco-editor": "^0.47.0",
    "monaco-editor-nginx": "^1.1.0",
    "monaco-types": "^0.1.0",
    "monaco-yaml": "^5.3.1",
    "prettier": "^3.5.3",
